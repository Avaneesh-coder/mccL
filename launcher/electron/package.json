{
	"name": "launcher",
	"version": "1.0.0",
	"description": "laucher made from electron",
	"main": "src/main.js",
	"scripts": {
		"start": "electron src/Main.js -dev",
		"build": "electron-packager . lighting-launcher --out=build  --overwrite --asar --icon=./icon.ico --ignore=.bat --ignore=.editorconfig --ignore=.gitignore --ignore=icon.ico --ignore=package-lock.json --ignore=README.md --ignore=.blend --ignore=.blend1 --ignore=.hdr"
	},
	"repository": {
		"type": "git",
		"url": "git+github.com/Avaneesh-coder/mccL.git"
	},
	"author": "LapisSea",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Avaneesh-coder/mccL/issues"
	},
	"homepage": "https://github.com/Avaneesh-coder/mccL/blob/main/launcher/electron/readme.txt",
	"win": {
		"target": "squirrel",
		"icon": "./icon.ico"
	},
	"dependencies": {
		"crypto-js": "^3.1.9-1",
		"electron-window-state": "^4.1.1",
		"shelljs": "^0.8.1"
	},
	"devDependencies": {
		"electron": "^1.8.4",
		"electron-packager": "^11.1.0"
	}
}
